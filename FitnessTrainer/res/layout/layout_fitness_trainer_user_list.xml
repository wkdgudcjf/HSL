<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F3F3" >

    <!-- 메인 Top Bar -->

    <include layout="@layout/layout_fitness_top_bar" />

    <!-- 회원 목록 리스트 -->

    <ListView
        android:id="@+id/fitness_trainer_user_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="118.25dp"
        android:layout_marginTop="70.00dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="118.25dp"
        android:layout_alignParentBottom="true" >

        <ImageButton
            android:id="@+id/fitness_trainer_user_register_button"
            android:layout_width="257.75dp"
            android:layout_height="41.00dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="47.25dp"
            android:src="@drawable/fitness_register_memeber_automatic_bt" />
    </RelativeLayout>

</RelativeLayout>