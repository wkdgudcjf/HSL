<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->

    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.NoActionBar.Fullscreen">

        <!-- API 14 theme customizations can go here. -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- 요일 -->
    <style name="calendar_day_of_week">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">22.25dp</item>
        <item name="android:textSize">16.5sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginLeft">23.5dp</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>

    <!-- 주 -->
    <style name="calendar_week">
        <item name="android:layout_height">75dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">7</item>
        <item name="android:background">@drawable/fitness_calendar_border</item>
    </style>
    
    <style name="calendar_trainer_week">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">7</item>
        <item name="android:layout_height">72dp</item>
        <item name="android:paddingLeft">19.75dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:background">@drawable/fitness_calendar_border</item>
    </style>
    

    <!-- 메인메뉴 아래 상태 레이블 글자 크기 -->
    <style name="style_fitness_main_range_heartrate">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">68.00dp</item>
        <item name="android:layout_height">19.50dp</item>
        <item name="android:layout_marginTop">5.75dp</item>
    </style>

    <style name="style_fitness_main_range_calorie">
